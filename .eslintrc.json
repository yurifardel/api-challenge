{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "eslint/consistent-type-definitions": "off",
    "eslint/strict-boolean-expressions": "off",
    "eslint/no-floating-promises": "off",
    "eslint/comma-spacing": "off",
    "eslint/return-await": "off",
    "eslint/no-namespace": "off",
    "eol-last": [
      "error",
      "never"
    ],
    "eslint/no-unused-vars": "off",
    "eslint/camelcase": "off",
    "no-unused-vars": "off",
    "no-redeclare": "off"
  }
}
